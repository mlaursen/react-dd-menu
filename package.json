{
  "name": "react-dd-menu",
  "version": "2.0.0",
  "description": "A react dropdown menu.",
  "main": "dist/index.js",
  "scripts": {
    "scripts": "babel ./src/js --out-dir ./dist",
    "styles": "sass --style expanded --line ./src/scss/react-dd-menu.scss ./dist/react-dd-menu.css && sass --style compressed ./src/scss/react-dd-menu.scss ./dist/react-dd-menu.min.css",
    "prebuild": "rimraf dist",
    "build": "npm run scripts && npm run styles",
    "postbuild": "postcss --use autoprefixer ./dist/*.css -d ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-dd-menu.git"
  },
  "keywords": [
    "react",
    "react-component",
    "dropown",
    "dropdown-menu",
    "component"
  ],
  "author": "mlaursen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-dd-menu/issues"
  },
  "homepage": "https://github.com/mlaursen/react-dd-menu#readme",
  "dependencies": {
    "classnames": "^2.1.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": ">= 15.3.0",
    "react-dom": ">= 15.3.0",
    "react-transition-group": "^1.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "postcss-cli": "^2.5.0",
    "prop-types": "^15.5.10",
    "react": "^15.4.0",
    "react-transition-group": "^1.1.3",
    "react-dom": "^15.4.0",
    "rimraf": "^2.5.1"
  }
}
